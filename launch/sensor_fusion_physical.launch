<launch>
  <node pkg="robot_localization" type="ekf_localization_node" name="foilboat_odom">
    <param name="frequency" value="50" />
    <param name="sensor_timeout" value="0.2" />
    <param name="two_d_mode" value="false" />
    <param name="publish_tf" value="true" />
    <param name="publish_acceleration" value="true" />

    <param name="map_frame" value="map" />
    <param name="odom_frame" value="odom" />
    <param name="base_link_frame" value="base_link" />
    <param name="world_frame" value="odom" />

    <param name="imu0" value="/vectornav/IMU" />
    <param name="imu0_differential" value="false"/>
    <param name="imu0_remove_gravitational_acceleration" value="true" />
    <rosparam param="imu0_config">
      [false, false, false,
      true, true, true,
      false, false, false,
      true, true, true,
      true, true, true]
    </rosparam>

    <param name="pose0" value="/foilboat/z_estimate" />
    <param name="pose0_differential" value="false" />
    <rosparam param="pose0_config">
      [false, false, true,
      false, false, false,
      false, false, false,
      false, false, false,
      false, false, false]
    </rosparam>

    <remap from="odometry/filtered" to="/foilboat/odometry/filtered/odom" />
  </node>

  <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform">
    <param name="frequency" value="10" />
    <param name="yaw_offset" value="-1.57" />

    <remap from="odometry/filtered" to="/plane_ros/odometry/filtered/odom" />
    <remap from="imu/data" to="/plane_ros/imu" />
    <remap from="gps/fix" to="/plane_ros/gps/fix" />
    <remap from="odometry/gps" to="/plane_ros/odometry/gps" />
  </node>

  <node pkg="robot_localization" type="ekf_localization_node" name="foilboat_map">
    <param name="frequency" value="5." />
    <param name="sensor_timeout" value="0.2" />
    <param name="two_d_mode" value="false" />
    <param name="publish_tf" value="true" />

    <param name="map_frame" value="map" />
    <param name="odom_frame" value="odom" />
    <param name="base_link_frame" value="base_link" />
    <param name="world_frame" value="map" />

    <param name="imu0" value="/vectornav/imu" />
    <param name="imu0_differential" value="false"/>
    <param name="imu0_remove_gravitational_acceleration" value="true" />
    <rosparam param="imu0_config">
      [false, false, false,
      true, true, true,
      false, false, false,
      false, false, false,
      true, true, true]
    </rosparam>

    <param name="pose0" value="/foilboat/z_estimate" />
    <param name="pose0_differential" value="false"/>
    <rosparam param="pose0_config">
      [false, false, true,
      false, false, false,
      false, false, false,
      false, false, false,
      false, false, false]
    </rosparam>

    <param name="odom0" value="/plane_ros/odometry/gps" />
    <param name="odom0_differential" value="false" />
    <rosparam param="odom0_config">
      [true, true, true,
      true, true, true,
      true, true, true,
      true, true, true,
      false, false, false]
    </rosparam>

    <remap from="odometry/filtered" to="/foilboat/odometry/filtered/map" />
  </node>
</launch>